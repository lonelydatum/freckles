{
  "name": "star-dust",
  "version": "0.0.0-development",
  "description": "make particles",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node server.js",
    "commit": "git-cz",
    "watch:test": "jest --watch",
    "test": "jest",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel src -d dist --copy-files --ignore '**/*.test.js'",
    "build:umd": "webpack --config webpack.config.babel.js --output-filename index.umd.js",
    "build:umd.min": "webpack --config webpack.config.babel.js --output-filename index.umd.min.js -p",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lonelydatum/star-dust.git"
  },
  "author": "Gar Liu <gar@lonelydatum.com> (http://lonelydatum.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lonelydatum/star-dust/issues"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "homepage": "https://github.com/lonelydatum/star-dust#readme",
  "keywords": [
    "random",
    "starwars"
  ],
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-polyfill": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-stage-2": "6.22.0",
    "chai": "3.5.0",
    "commitizen": "2.9.5",
    "cz-conventional-changelog": "1.2.0",
    "jest-cli": "18.1.0",
    "mocha": "3.2.0",
    "npm-run-all": "4.0.1",
    "rimraf": "2.5.4",
    "semantic-release": "^6.3.2",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.2.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "dependencies": {
    "computed-style": "0.3.0",
    "lodash": "4.17.4",
    "rasterizehtml": "1.2.4",
    "text-height": "1.0.2",
    "to-css": "1.2.1",
    "webfontloader": "1.6.27"
  }
}
